{
  "name": "@caiquecamargo/utils",
  "version": "0.1.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    "./composables": {
      "import": "./dist/composables/index.mjs",
      "require": "./dist/composables/index.cjs",
      "types": "./dist/composables/index.d.ts",
      "default": "./dist/composables/index.mjs"
    },
    "./core": {
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.cjs",
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.mjs"
    },
    "./web": {
      "import": "./dist/web/index.mjs",
      "require": "./dist/web/index.cjs",
      "types": "./dist/web/index.d.ts",
      "default": "./dist/web/index.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caiquecamargo/utils.git"
  },
  "author": "Caique de Camargo",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "npx zx scripts/generator.mjs",
    "pub": "npx zx scripts/publish.mjs",
    "prepare": "ts-patch install"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^18.17.3",
    "@vitest/coverage-v8": "^0.34.1",
    "happy-dom": "^10.8.0",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "typescript": "^5.1.6",
    "typia": "^4.1.16",
    "vite": "^4.4.8",
    "vitest": "^0.34.1",
    "zx": "^7.2.3"
  },
  "keywords": [],
  "license": "ISC",
  "typesVersions": {
    "*": {
      "composables": [
        "dist/composables/index.d.ts"
      ],
      "core": [
        "dist/core/index.d.ts"
      ],
      "web": [
        "dist/web/index.d.ts"
      ]
    }
  },
  "packageManager": "pnpm@8.6.11",
  "engines": {
    "node": "16.20.0"
  }
}
